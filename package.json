{
  "name": "motomenus-saas-ui",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@reach/router": "^1.2.1",
    "animate.css": "^3.7.0",
    "autoprefixer": "^9.7.3",
    "axios": "^0.19.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.0.1",
    "dotenv-webpack": "^1.6.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass": "^1.24.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.1.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run dev:prettier:format && npm run dev:eslint:check"
    }
  },
  "scripts": {
    "dev:eslint:check": "eslint \"src/**/*.{js,jsx}\"",
    "dev:prettier:format": "prettier --write \"src/**/*.{css,js,json,jsx}\"",
    "dev:tailwind-cli:build": "npx tailwind build ./src/sass/tailwind-source.scss -c tailwind.config.js -o src/css/tailwind-output.css",
    "dev:webpack:build": "webpack --env.mode development",
    "dev:webpack:debug": "webpack --env.mode development --inspect --debug-brk",
    "dev:webpack:server": "webpack-dev-server --env.mode development --hot --inline",
    "dev:webpack:watch": "webpack --env.mode development --watch",
    "prod:eslint:check": "eslint \"src/**/*.{js,jsx}\"",
    "prod:prettier:check": "prettier --list-different \"src/**/*.{css,js,json,jsx}\"",
    "prod:webpack:build": "webpack --env.mode production"
  }
}
